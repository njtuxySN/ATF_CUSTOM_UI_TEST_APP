        <client_script><![CDATA[	
	function runIsReadonlyCommand(mugshot){
		return window.top.g_automate.isReadOnly(mugshot);
	}

	function runClickCommand(mugshot){
		return window.top.g_automate.click(mugshot);
	}

	function runSetValueCommand(mugshot, value) {
		return window.top.g_automate.setValue(mugshot, value)
	}
		
	function runGetValueCommand(mugshot, value){
		return window.top.g_automate.getValue(mugshot)		
	}
		
    
    // The corresponding html page should have component ID like: i_button, i_button_readonly, i_date_disabled, etc
    function get_mugshot_id_of_component(input_type, extra_attribute){
    	if(extra_attribute ==""){
		    return "{\"id\":\"i_" + input_type + "\"}"
    	}else{
			return "{\"id\":\"i_" + input_type + "_" + extra_attribute+ "\"}"
    	}    	
    }		
		

	//input type, default value, new value
	var regular_setttable_components = [
	    ['checkbox', 'true', 'false'],
		['color', '#e66465', '#000000'],
		['date', '2018-07-22', '2028-07-22'],
		['datetime_local', '2018-06-12T19:30', '2028-06-12T19:30'],
		['email', 'drone@globex.com', 'test@servicenow.com'],
		['month', '2018-05', '2028-05'],
		['number', '0.2', '0.9'],
		['password', 'defaultPassword', 'new password'],
		['range', '3', '5'],
		['search', 'search somthing', 'search something new'],
		['tel', '415-666-8888', '999-000-1234'],
		['text', 'regular text', 'new regular text'],
		['time', '02:02', '03:03'],
		['url', 'https://defaultURL.com', 'https://www.servicenow.com'],
		['week', '2018-W16', '2028-W16'],
		['select', 'cherry', 'apple'],
		['text_area', 'Textarea with sizing and wrap attribute (try values of hard, soft, and off to see how it affects wrapping). The maximum number of characters is constrained to 200 by the maxlength attribute.', 'new value for text area']
	];
		

	for(var i=0; i<regular_setttable_components.length; i++){
		var input_type = regular_setttable_components[i][0];
		var default_value = regular_setttable_components[i][1];
		var new_value = regular_setttable_components[i][2];
		runTest(input_type, default_value, new_value )				
				
	}

	function runTest(input_type, default_value, new_value ){
		it("input type: " + input_type + " should be settable", function() {
			var mugshot = get_mugshot_id_of_component(input_type, "");
			var response = runSetValueCommand(mugshot, new_value);
			expect(response.success).toBe(true, input_type + " is expected be settable: but set the value but response returns " + response.success);
			var response = runGetValueCommand(mugshot);
			expect(response.result).toBe(new_value, new_value + " is expected after set the value but the result is: " + response.result);
		});			
	}
		
]]></client_script>
